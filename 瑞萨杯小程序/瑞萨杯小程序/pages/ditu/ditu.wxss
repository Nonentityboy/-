/* pages/ditu/ditu.js.wxss */
.contain{
  display:flex;
  flex-direction:column;
  width:100%;
  height:100vh;
  }
 .img{
   width:60rpx;
   height:60rpx;
   border-radius:50%;
 }
.userinfo{
  width:50rpx;
  height:50rpx;
  margin:70rpx 35rpx 20rpx 50rpx;
}
.tip-k{
  display:flex;
  flex-direction:row;
  background-color:#ffffff;
  width:560rpx;
  height:35px;
  margin-top:10px;
  margin-left:100rpx;
  border:1px solid #c0c0c0;
  border-radius:5px;
  vertical-align:-webkit-baseline-middle;
}
.sbc{
  font-size: 30rpx;
  text-align:center;
}
.searchcion{
margin-top:15rpx;
z-index:2;
width:15px;
height:25px;
margin-left:20rpx;
}
.image-right{
  width:80rpx;
  height:50rpx;
  margin:7rpx 20rpx 20rpx 20rpx;
  vertical-align:middle;
}
.map_container{
  display:flex;
  flex-direction:row;
  width:100%;
  position: absolute;
  bottom:1px;
  top: 0px;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.cover{
  width:300rpx;
  height:300rpx;
}
.cover-img{
  position:absolute;
  left:250rpx;
  bottom:20rpx;
  width:300rpx;
  height:250rpx;
}
.dw{
  width:50rpx;
  height:50rpx;
}
.dw-img{
  position:absolute;
  left:30rpx;
  bottom:40rpx;
  width:70rpx;
  height:70rpx;
}
.co{
  width:50rpx;
  height:50rpx;
}
.co-img{
  position:absolute;
  right:30rpx;
  bottom:40rpx;
  width:70rpx;
  height:70rpx;
}
